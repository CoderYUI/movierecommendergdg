{
  "version": 2,
  "builds": [
    {
      "src": "model/app.py",
      "use": "@vercel/python"
    },
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "dist" }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "model/app.py"
    },
    {
      "src": "/(.*)",
      "dest": "/dist/$1"
    }
  ]
}